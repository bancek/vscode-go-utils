{
	"name": "go-utils",
	"displayName": "go-utils",
	"description": "Golang utils",
	"publisher": "lukazakrajsek",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/bancek/vscode-go-utils"
	},
	"licenses": [
		{
			"type": "MIT",
			"url": "https://raw.githubusercontent.com/bancek/vscode-go-utils/master/LICENSE.md"
		}
	],
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:go-utils.breakLine"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "go-utils.breakLine",
				"title": "Go utils: Break line"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	}
}